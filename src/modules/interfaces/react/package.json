{
  "name": "boo-autoagent-cli",
  "version": "0.1.3",
  "description": "Boo-AutoAgent CLI with React + Ink",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "boo-react": "dist/index.js",
    "boo-native": "dist/index-native.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node --expose-gc --max-old-space-size=6144 dist/index.js",
    "dev": "node --expose-gc --max-old-space-size=6144 --loader tsx src/index.tsx",
    "typecheck": "tsc --noEmit",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs",
    "test:watch": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs --watch",
    "test:coverage": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs --coverage",
    "test:setup": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs setup.test.tsx",
    "test:operations": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs operation-flow.test.tsx",
    "test:visual": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs visual-regression.test.tsx",
    "test:journeys": "NODE_OPTIONS=--experimental-vm-modules jest --config tests/jest.config.cjs --verbose",
    "test:all": "npm run build && npm run test:coverage",
    "validate": "npm run build && npm run test:all",
    "collab:migrate": "tsx ../../../collaboration/database/migrate.ts",
    "collab:server": "tsx ../../../collaboration/server/index.ts"
  },
  "dependencies": {
    "@types/chokidar": "^1.7.5",
    "@types/yaml": "^1.9.6",
    "ansi-escapes": "^7.0.0",
    "bcrypt": "^5.1.1",
    "chokidar": "^4.0.3",
    "commander": "^11.1.0",
    "cors": "^2.8.5",
    "dockerode": "^4.0.2",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "ink": "^4.4.1",
    "ink-big-text": "^2.0.0",
    "ink-gradient": "^3.0.0",
    "ink-link": "^4.1.0",
    "ink-progress-bar": "^3.0.0",
    "ink-select-input": "^4.2.2",
    "ink-spinner": "^4.0.3",
    "ink-text-input": "^4.0.3",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^9.0.2",
    "meow": "^12.1.1",
    "node-pty": "^1.0.0",
    "pg": "^8.11.3",
    "react": "^18.3.1",
    "redis": "^4.6.12",
    "strip-ansi": "^7.1.0",
    "uuid": "^9.0.1",
    "ws": "^8.16.0",
    "yaml": "^2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@jest/globals": "^29.7.0",
    "@types/dockerode": "^3.3.5",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.10.9",
    "@types/react": "^18.3.0",
    "@types/react-test-renderer": "^18.3.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "babel-jest": "^29.7.0",
    "chalk": "^5.4.1",
    "ink-testing-library": "^4.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdom": "^26.1.0",
    "react-dom": "^18.3.1",
    "react-test-renderer": "^18.3.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.6.2",
    "typescript": "^5.9.2"
  }
}
