# Docker Compose Environment Variables
# Copy this file to .env and fill in real values

# NextAuth Secret (generate with: openssl rand -base64 32)
NEXTAUTH_SECRET=generate-a-strong-secret-here

# Langfuse Encryption (generate with: openssl rand -hex 32)
LANGFUSE_ENCRYPTION_KEY=generate-64-char-hex-string-here
LANGFUSE_SALT=generate-salt-here

# Redis Password (generate with: openssl rand -base64 24)
REDIS_PASSWORD=generate-strong-redis-password

# Database Password (generate with: openssl rand -base64 24)
COLLAB_DB_PASSWORD=generate-strong-db-password

# AWS Configuration (if using)
AWS_REGION=us-east-1
AWS_ACCOUNT=123456789012